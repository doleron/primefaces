<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0">

    <namespace>http://primefaces.org/ai</namespace>

    <tag>
        <description>
            <![CDATA[.]]>
        </description>
        <tag-name>smartCam</tag-name>
        <component>
            <component-type>org.primefaces.component.ai.smartcam.SmartCam</component-type>
            <renderer-type>org.primefaces.component.ai.smartcam.SmartCamRenderer</renderer-type>
            <handler-class>org.primefaces.component.ai.smartcam.SmartCamHandler</handler-class>
        </component>
        <attribute>
            <description>
                <![CDATA[Unique identifier of the component in a namingContainer.]]>
            </description>
            <name>id</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Boolean value to specify the rendering of the component, when set to false component will not be rendered.]]>
            </description>
            <name>rendered</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Value of the component.]]>
            </description>
            <name>value</name>
            <required>false</required>
            <type>java.lang.Object</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Marks component as required.]]>
            </description>
            <name>required</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[A method binding expression referring to a method for handling a valuchangeevent.]]>
            </description>
            <name>valueChangeListener</name>
            <required>false</required>
            <type>javax.faces.event.ValueChangeListener</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Message to display when required field validation fails.]]>
            </description>
            <name>requiredMessage</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Name of the client side widget.]]>
            </description>
            <name>widgetVar</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Style of the camera container.]]>
            </description>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Style class of the camera container.]]>
            </description>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Method expression to listen capture events.]]>
            </description>
            <name>listener</name>
            <required>false</required>
            <type>javax.el.MethodExpression</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Width of the camera viewport.]]>
            </description>
            <name>width</name>
            <required>false</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Height of the camera viewport.]]>
            </description>
            <name>height</name>
            <required>false</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Disable camera start after initialize]]>
            </description>
            <name>autoStart</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[video update timeout in milliseconds.]]>
            </description>
            <name>renderTimeout</name>
            <required>false</required>
            <type>java.lang.Integer</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[callback called when detections are caught]]>
            </description>
            <name>renderTimeout</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[constructor function for the model used to make predictions]]>
            </description>
            <name>model</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <description>
                <![CDATA[function called to make predictions]]>
            </description>
            <name>predict</name>
            <required>false</required>
            <type>java.lang.String</type>
        </attribute>
    </tag>

</facelet-taglib>
